{
  "name": "vscode-mcd-helper",
  "displayName": "VscodeMcdHelper",
  "version": "0.0.2",
  "description": "vscode extension for mcd",
  "repository": {
    "type": "git",
    "url": "https://github.com/tangdaoyuan/vscode-mcd-helper",
    "directory": "extension"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "publisher": "Tedy",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mcd.now",
    "onCommand:mcd.debug",
    "onCommand:mcd.release",
    "onCommand:mcd.production"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mcd.now",
        "title": "MCD Now"
      },
      {
        "command": "mcd.debug",
        "title": "mcd goto Debug"
      },
      {
        "command": "mcd.test",
        "title": "mcd goto Test"
      },
      {
        "command": "mcd.release",
        "title": "mcd goto Release"
      },
      {
        "command": "mcd.production",
        "title": "mcd goto Production"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.20",
    "@types/semver": "^7.3.9",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.17.0",
    "glob": "^8.0.3",
    "mocha": "^9.2.2",
    "semver": "^7.3.7",
    "typescript": "^4.7.3"
  }
}
