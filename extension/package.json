{
  "name": "vscode-mcd-helper",
  "displayName": "VscodeMcdHelper",
  "version": "0.1.0",
  "description": "vscode extension for mcd",
  "repository": {
    "type": "git",
    "url": "https://github.com/tangdaoyuan/vscode-mcd-helper",
    "directory": "extension"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "publisher": "Tedy",
  "categories": [
    "Other"
  ],
  "icon": "res/logo.png",
  "activationEvents": [
    "onCommand:mcd.author",
    "onCommand:mcd",
    "onCommand:mcd.debug",
    "onCommand:mcd.test",
    "onCommand:mcd.release",
    "onCommand:mcd.production",
    "onView:mcdDeployHelperView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "mcdDeployHelperView",
          "name": "MCD Deployment Helper"
        }
      ]
    },
    "commands": [
      {
        "command": "mcd.author",
        "title": "MCD EX Author"
      },
      {
        "command": "mcd",
        "title": "MCD Open (Auto Detect)"
      },
      {
        "command": "mcd.debug",
        "title": "MCD Open Debug"
      },
      {
        "command": "mcd.test",
        "title": "MCD Open Test"
      },
      {
        "command": "mcd.release",
        "title": "MCD Open Release"
      },
      {
        "command": "mcd.production",
        "title": "MCD Open Production"
      }
    ],
    "configuration": {
      "mcd.env": {
        "title": "MCD Environments Key",
        "type": "array",
        "default": [
          "debug",
          "test",
          "release",
          "production"
        ],
        "description": "MCD Environments Key"
      }
    }
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}
